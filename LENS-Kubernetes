# https://docs.k8slens.dev/getting-started/install-lens/#debian

# https://api.k8slens.dev/binaries/latest.x86_64.AppImage

# This will be portable lens installation


apt update -y
apt install -y snapd fuse
curl -LO -# https://api.k8slens.dev/binaries/latest.x86_64.AppImage
chmod +x latest.x86_64.AppImage
./latest.x86_64.AppImage
