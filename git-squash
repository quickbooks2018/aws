git log --oneline

############################
# Git log with commiter name
############################
git log --oneline --format="%h %an %s"

git log --format="%h %an %s"

git log --oneline --format="%h %an %s" -n13

git log --oneline -n13

git rebase -i HEAD~13

from bottom ---> s  (squash)

git branch

git push origin branchname -f
